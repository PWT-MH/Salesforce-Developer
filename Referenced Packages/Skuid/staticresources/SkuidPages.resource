ditions></action><action type=\"delete\"><renderconditions logictype=\"and\"><rendercondition type=\"multiple\" operator=\"not in\" field=\"Name\" value=\"\" fieldmodel=\"Themes_Desktop\" sourcetype=\"fieldvalue\" enclosevalueinquotes=\"true\"><values><value>Classic</value><value>Modern</value><value>Paper</value></values></rendercondition></renderconditions><drawer title=\"Drawer Area\" width=\"90%\" closehandle=\"true\"><components/></drawer></action></rowactions><massactions usefirstitemasdefault=\"true\"/><views><view type=\"standard\"/></views></skootable></components></tab><tab name=\"Mobile\" loadlazypanels=\"true\"><components><skootable showconditions=\"true\" showsavecancel=\"false\" searchmethod=\"server\" searchbox=\"false\" showexportbuttons=\"false\" pagesize=\"10\" createrecords=\"true\" model=\"Themes_Mobile\" buttonposition=\"\" mode=\"read\"><fields><field id=\"Name\"/><field id=\"skuid__Resource_Name__c\"/><field id=\"skuid__Resource_Namespace__c\"/><field id=\"skuid__Active__c\"/></fields><rowactions><action type=\"edit\"><renderconditions><rendercondition type=\"fieldvalue\" operator=\"!=\" enclosevalueinquotes=\"true\" field=\"Name\" value=\"MobileClassic\"/></renderconditions></action><action type=\"delete\"><renderconditions logictype=\"and\"><rendercondition type=\"fieldvalue\" operator=\"!=\" field=\"Name\" value=\"MobileClassic\" enclosevalueinquotes=\"true\"/></renderconditions><drawer title=\"Drawer Area\" width=\"90%\" closehandle=\"true\"><components/></drawer></action></rowactions><massactions usefirstitemasdefault=\"true\"/><views><view type=\"standard\"/></views></skootable></components></tab></tabs></tabset></components></tab></tabs></tabset></components><resources><labels/><javascript><jsitem location=\"inline\" name=\"Custom Renderers\" cachelocation=\"false\" url=\"\">(function(skuid){\n    \n\tvar $ = skuid.$,\n\t    registerSnippet = skuid.snippet.registerSnippet;\n\t    \n\t// Base snippet for ThemePicker picklists\n    var themePicker = function(config){\n        \n        var themeType = config.type,\n            field = config.field,\n            value = skuid.utils.decodeHTML(config.value),\n            filterFunction = function(theme){\n                return (theme.skuid__Type__c === themeType);\n            },\n            isRequired = config.isRequired,\n            themes = skuid.model.getModel('Themes_'+themeType);\n        \n        if (field.mode === 'edit') {\n            \n            field.element.html(skuid.ui.renderers.PICKLIST.edit({\n                value: value,\n                required: isRequired,\n                onChange: function(value){\n                    field.model.updateRow(field.row,field.id,value);\n                },\n                entries: $.map(themes.data.filter(filterFunction),function(item){\n                    return {\n                        label: item.Name,\n                        value: item.Name\n                    };  \n                })\n            }));\n        } else {\n            skuid.ui.fieldRenderers[field.metadata.displaytype][field.mode](field,value);\n        }\n            \n        \n    };\n    \n    // Desktop ThemePicker\n    registerSnippet('ThemePicker_Desktop',function(field,value){\n        themePicker({\n            field:field,\n            value:value,\n            isRequired:true,\n            type: 'Desktop'\n        });\n    });\n    \n    // Mobile ThemePicker\n    registerSnippet('ThemePicker_Mobile',function(field,value){\n        themePicker({\n            field:field,\n            value:value,\n            isRequired:true,\n            type: 'Mobile'\n        });\n    });\n    \n    // Theme Type Picker\n    registerSnippet('ThemeTypePicklist',function(){\n        \n        var field = arguments[0],\n            value = skuid.utils.decodeHTML(arguments[1]);\n        \n        if (field.mode === 'edit') {\n            \n            field.element.html(skuid.ui.renderers.PICKLIST.edit({\n                value: value,\n                required: true,\n                onChange: function(value){\n                    field.model.updateRow(field.row,field.id,value);\n                },\n                entries: [\n                    {label:'Desktop',value:'Desktop'},\n                    {label:'Mobile',value:'Mobile'}\n                ]\n            }));\n        } else {\n            skuid.ui.fieldRenderers[field.metadata.displaytype][field.mode](field,value);\n        }\n        \n    });\n    \n    // Component Pack Type picker\n    registerSnippet('ComponentPackType',function(){\n        \n        var field = arguments[0],\n            value = skuid.utils.decodeHTML(arguments[1]),\n            entries = [\n                {label:'Any',value:'Any'},\n                {label:'Desktop only',value:'Desktop'},\n                {label:'Mobile only',value:'Mobile'}\n            ];\n        \n        if (field.mode === 'edit') {\n            field.element.html(skuid.ui.renderers.PICKLIST.edit({\n                value: value,\n                required: true,\n                onChange: function(value){\n                    field.model.updateRow(field.row,field.id,value,{ initiatorId: field._GUID });\n                },\n                entries: entries\n            }));\n        } else {\n            // If we find our value in our entries, use the entry's Label as our display value\n            $.each(entries,function(){\n               if (value === this.value) {\n                   value = this.label;\n                   return false;\n               } \n            });\n            skuid.ui.fieldRenderers[field.metadata.displaytype][field.mode](field,value);\n        }\n        \n    });    \n    \n    var textareaMultipicklist = function(field,value,picklistEntries) {\n    \n        value = skuid.utils.decodeHTML(arguments[1]);\n        \n        var metadata = {};\n        $.extend(true,metadata,field.metadata); // do a deep clone\n        field.metadata = metadata;\n        \n        metadata.picklistEntries = picklistEntries;\n        skuid.ui.fieldRenderers.MULTIPICKLIST[field.mode](field,value);\n    };\n    \n    \n    registerSnippet('PackagesMultipicklist',function(field,value){\n        \n        var picklistEntries = [];\n        \n        $.each(skuid.$M('Packages').data,function(i,p){\n            picklistEntries.push({ \n                value: p.NamespacePrefix, \n                label: p.NamespacePrefix, \n                active: true, \n                defaultValue: i===0\n            });\n        });\n        \n        textareaMultipicklist(field,value,picklistEntries); \n    });\n    \n    registerSnippet('PermissionSetsMultipicklist',function(field,value){\n        \n        var picklistEntries = [];\n        \n        $.each(skuid.$M('PermissionSets').data,function(i,p){\n            var un = (p.NamespacePrefix ? p.NamespacePrefix + '__' : '') + p.Name;\n            var label = p.Label + (p.NamespacePrefix ? ' (' + p.NamespacePrefix + ')' : '');\n            picklistEntries.push({\n                value: un, \n                label: label, \n                active: true, \n                defaultValue: i===0\n            });\n        });\n        \n        textareaMultipicklist(field,value,picklistEntries); \n    });\n    \n    \n    // polymorphic lookup\n    var limitedLookup = function(field,value,targetObjects,renderAsPicklist){\n        \n        value = skuid.utils.decodeHTML(value);\n        \n        var metadata = field.metadata;\n        \n        if (field.mode == 'edit') {\n            \n            metadata = {};\n            $.extend(true,metadata,field.metadata); // do a deep clone\n            field.metadata = metadata;\n            \n            // Limit the set of target objects\n            var targets = [],\n                uniqueTargets = {};\n            $.each(metadata.referenceTo,function(i,r){\n                if (($.inArray(r.objectName,targetObjects) != -1)\n                &amp;&amp; (!uniqueTargets[r.objectName])) {\n                    targets.push(r);\n                    uniqueTargets[r.objectName] = 1;\n                    if (targets.length == targetObjects.length) return false;\n                }\n            });\n            if (targets.length) {\n                // Make this field render as a picklist?\n                if (renderAsPicklist) field.options.type = 'REFPICK';\n                // Override the current referenceTo\n                metadata.referenceTo.length = 0;\n                var ref = [];\n                $.each(targets,function(){\n                \tref.push(this.objectName);\n                });\n                metadata.ref = ref.join();\n                metadata.referenceTo = targets;\n            }\n            \n        }\n        \n        // Run the standard renderer\n        skuid.ui.fieldRenderers[metadata.displaytype][field.mode](field,value);\n        // Set the field renderer BACK to custom so that our renderer will work correctly\n        // the second time\n        if (renderAsPicklist) field.options.type = 'CUSTOM';    \n    };\n    \n    // Profile Lookup\n    registerSnippet('ProfileLookup',function(field,value){\n        limitedLookup(field,value,['Profile'],false);\n    });\n    // User/Profile Lookup\n    registerSnippet('UserOrProfileLookup',function(field,value){\n        limitedLookup(field,value,['User','Profile'],false);\n    });\n    \n    // Session Timeout picklist field renderer (not currently used)\n\t/*\n\tregisterSnippet('skuid.sessionTimeoutPicker',function(){\n\t    var field = arguments[0],\n            value = arguments[1];\n            \n        if (!value) {\n            value = 120;    \n            field.model.updateRow(field.row,field.id,value,{initiatorId: field._GUID});\n        }\n        \n        var getValueLabel = function(val){\n          if (val &lt; 60) {\n              return val + ' minutes';\n          } else {\n              return Math.round(val / 60) + ' hours';\n          }  \n        };\n            \n        if (field.mode !== 'edit') {\n            skuid.ui.fieldRenderers.TEXT[field.mode](field,getValueLabel(field.row[field.id]));\n        } else {\n            var entries = $.map([15,30,45,60,120,240,480,720],function(val){\n                return {\n                    label: getValueLabel(val),\n                    value: val\n                };\n            });\n            field.element.append(skuid.ui.renderers.PICKLIST.edit({\n               entries: entries,\n                required: true,\n                onChange: function(val){\n                    field.model.updateRow(field.row,field.id,parseInt(val,10),{initiatorId: field._GUID});\n                },\n                value: value\n            }));\n            \n        } \n\t});\n    */    \n    \n\t\n})(skuid);</jsitem><jsitem location=\"inline\" name=\"Snippets\" cachelocation=\"false\" url=\"\">(function(skuid){\n    \n\tvar $ = skuid.$,\n\t    registerSnippet = skuid.snippet.registerSnippet;\n\t\n\t//\n\t// Utility functions\n\t//\n\t\n    var utf8ToBase64 = function(str){\n        return window.btoa(window.unescape(encodeURIComponent( str )));\n    };\n    \n    // JSON-stringifies a Resource body but pretty-prints it\n    var toJSON = function(resourceBody){\n        return JSON.stringify(resourceBody, undefined, 3); // three spaces for indents\n    };\n\t\n\t// Register common snippets\n\tregisterSnippet('skuid.loadJSZip',function(){\n\t    skuid.utils.loadJS('/resource/skuid__JSZip');\n\t});\n\t\n\t// Pre-save logic run when creating a new Component Pack\n\tregisterSnippet('skuid.newComponentPackPreSave',function(){\n\t    var params = arguments[0],\n        \t$ = skuid.$,\n            dfd = $.Deferred(),\n            row = params.row,\n            model = params.model;\n        \n        var resourceNamespace = row.skuid__Resource_Namespace__c;\n        var packLabel = row.skuid__Resource_Name__c;\n        var packIcon = \"sk-icon-component-pack\";\