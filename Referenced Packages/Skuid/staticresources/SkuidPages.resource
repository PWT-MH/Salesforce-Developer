rials and documentation'),$('&lt;div&gt;').prop('id','newsfeed'));\n\n// Set up the dive in area\ndivein.append($('&lt;h2&gt;').text('dive in').css('margin-top','0px'));\ndivein.append(linkwrapper);\n\n$.each(helplinks,function() {\n    var link = $('&lt;li&gt;').html('&lt;a href=\"' + this.link + '\" target=\"_blank\"&gt;' + this.title + '&lt;/a&gt;');\n    linkwrapper.append(link);\n});\n\n// Set up the social area\ndivein.append($('&lt;h2&gt;').text('get social'));\ndivein.append(socialwrapper);\n\n$.each(sociallinks,function() {\n    var social = $('&lt;a&gt;').prop('href',this.link).prop('target','_blank').html('&lt;img src=\"https://skuid.secure.force.com/resource/1360695993000/SocialIcons/' + this.iconname + '\" class=\"socialicon\" alt=\"' + this.title + '\"&gt;&lt;/img&gt;');\n    socialwrapper.append(social);\n});\n\n// Set up \"leave us a review\" area\nreview.append($('&lt;h2&gt;').text('show skuid some love').css('margin-top','0px'));\nreview.append($('&lt;a&gt;').prop('href','https://appexchange.salesforce.com/listingDetail?listingId=a0N30000009wyDjEAI').prop('target','_blank').text('Write a review on the AppExchange.'));\n\n\ntoprow.append(divein,newsfeedarea);\nbottomrow.append(review, resourcesarea);\nresourcesarea.append($('&lt;h1&gt;').text('skuid resources'));\n\n$.each(resources,function() {\n    var resourcewrapper = $('&lt;div&gt;').addClass('resourcecontainer');\n    resourcewrapper.html('&lt;a href=\"' + this.link + '\" target=\"_blank\"&gt;&lt;h3&gt;&lt;a href=\"' + this.link + '\" target=\"_blank\"&gt;' + this.title + '&lt;/a&gt;&lt;/h3&gt;&lt;p&gt;' + this.desc + '&lt;/p&gt;');\n    resourcesarea.append(resourcewrapper);\n});\n\nelem.append(toprow,bottomrow);\n\nvar feedItems;\n    \nfunction checkFeeds() {    \t\t\t\n\t\n\tvar feedItems = [],\n\t    feedsLoaded = [];\n\n\tfeedsLoaded.news = false;\n\tfeedsLoaded.help = false; \t\t\t\t\t\n\t\n\t// Skuid News Feed Items Feed \n\tparseRSS(\"https://skuid.secure.force.com/services/xml/SkuidCentral\", function(feed) {\n\t\t\t\t\t\n\t\t$.each(feed.entries,function(i, item){\t\t\t\t\n\t\t\tfeedItems.push(item);\t\t\t\t\n\t\t});\n\t\t\n\t\tfeedsLoaded.news = true;\n\t\t\n\t\tif(feedsLoaded.news &amp;&amp; feedsLoaded.help) { \n\t\t\tmakeNewsItems(feedItems);\n\t\t}\n\t});\n\t\n\t// Skuid Tutorials Feed \n\tparseRSS(\"http://help.skuidify.com/s/1895.rss\", function(feed) {\t\t\t\n\t\t\t\t\n\t\t$.each(feed.entries,function(i, item){\n\t\t\titem.title =  item.title;\n\t\t\tfeedItems.push(item);\t\t\t\t\n\t\t});\t\t\t\n\n\t\tfeedsLoaded.help = true;\n\t\t\n\t\tif(feedsLoaded.news &amp;&amp; feedsLoaded.help) { \n\t\t\tmakeNewsItems(feedItems);\n\t\t}\n\t});\n\t\n}\t\n\nfunction parseRSS(url, callback) {\n\t$.ajax({\n\t\turl: document.location.protocol + '//ajax.googleapis.com/ajax/services/feed/load?v=1.0&amp;num=10&amp;callback=?&amp;q=' + encodeURIComponent(url),\n\t\tdataType: 'json',\n\t\tsuccess: function(data) {\n\t\t  callback(data.responseData.feed);\n\t\t}\n\t});\n}\n\nfunction makeNewsItems(feedItems) {\n\t\t\t\t\n\t$('#newsfeed').empty();\n\t\n\tvar itemsToShow = 4;\n\t\n\t// sort the array by the published date \n\tfeedItems.sort(function(a,b){\n\t\ta = new Date(a.publishedDate);\n\t\tb = new Date(b.publishedDate);\n\t\treturn b-a; // descending \n\t});\n\t\t\t\n\tif(feedItems.length &gt; itemsToShow) { \n\t\tfeedItems = feedItems.slice(0,itemsToShow); \n\t}\n\t\t\t\t\t\t\t\t\n\t$.each(feedItems, function(i, item){\t\n\t\tvar link = item.link; \n\t\tvar title = '&lt;h3&gt;' + item.title + '&lt;/h3&gt;';\n\t\tif(link) {\n\t\t\ttitle = '&lt;a href=\"' + link + '\" target=\"_blank\"&gt;' + title + '&lt;/a&gt;';\n\t\t} \n\t\t\t\n\t\tvar content = '&lt;p&gt;' + item.contentSnippet + '&lt;/p&gt;';\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\n\t\tvar wrapper = $('&lt;div&gt;').addClass('newsitem').html(title + content).appendTo('#newsfeed');\t\t\t\n\t});\n    skuidpage.show();\n}\t\n\t\n\ncheckFeeds();</jsitem></javascript><css><cssitem location=\"inline\" 